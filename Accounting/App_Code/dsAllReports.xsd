            <MainSource>
              <DbSource ConnectionRef="Constring (Web.config)" DbObjectType="Unknown" FillMethodModifier="Public" FillMethodName="Fill" GenerateMethods="Both" GenerateShortCommands="false" GeneratorGetMethodName="GetData" GeneratorSourceName="Fill" GetMethodModifier="Public" GetMethodName="GetData" QueryType="Rowset" ScalarCallRetval="System.Object, mscorlib, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" UseOptimisticConcurrency="false" UserGetMethodName="GetData" UserSourceName="Fill">
                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>
                      /*select fin.[Type],fin.Sub, AccNo,AccountName as [Description],-(  case when (Bal) &lt;=  0 then Bal else null end)  as 'Debit', case when (Bal)  &gt;0 then Bal  end as Credit,tblTrxn.Bal as Balance from (
                      select  case when ISNUMERIC(Account)=1 then '213' when ISNUMERIC(Account)=0 then left(account,CHARINDEX('/',account)-1) end as AccNo,sum(debit) as Debit,sum(credit) as Credit,sum(credit)-sum(debit) as Bal from Accounts_Transactions where TrxnDate &lt;= @tDate group by case when ISNUMERIC(Account)=1 then '213' when ISNUMERIC(Account)=0 then left(account,CHARINDEX('/',account)-1) end
                      )tblTrxn join
                      tbl_FinancialAccountsCreation fin on tblTrxn.AccNo=fin.MainAccount
                      where fin.subaccount=1 and Type='Income' order by accno*/

                      select tblTrxn.TrxnDate,tblTrxn.TrxnDesc, fin.[Type],fin.Sub,subCat.SubType,sc.Category,sc.Statement, AccNo,tblTrxn.SubAccNo,AccountName as [Description],-(  case when (Bal) &lt;=  0 then Bal else null end)  as 'Debit', case when (Bal)  &gt;0 then Bal  end as Credit,tblTrxn.Bal as Balance from (
                      select TrxnDate,Description as TrxnDesc, case when ISNUMERIC(Account)=1 then [AccountPrefix] when ISNUMERIC(Account)=0 then left(account,CHARINDEX('/',account)-1) end as AccNo,case when ISNUMERIC(Account)=1 OR left(account,CHARINDEX('/',account)-1)=[AccountPrefix] then '2' ELSE right(account,len(account)-CHARINDEX('/',account)) end as SubAccNo,sum(debit) as Debit,sum(credit) as Credit,sum(credit)-sum(debit) as Bal from Accounts_Transactions join ParaInternalControls pic on 1=1 where TrxnDate&gt;=@fDate AND TrxnDate &lt;= @tDate group by case when ISNUMERIC(Account)=1 then [AccountPrefix] when ISNUMERIC(Account)=0 then left(account,CHARINDEX('/',account)-1) end,case when ISNUMERIC(Account)=1 OR left(account,CHARINDEX('/',account)-1)=[AccountPrefix] then '2' ELSE right(account,len(account)-CHARINDEX('/',account)) end
                      ,trxndate,Description)tblTrxn join
                      tbl_FinancialAccountsCreation fin on tblTrxn.AccNo=fin.MainAccount AND tblTrxn.SubAccNo=fin.SubAccount
                      LEFT JOIN tbl_FinancialCategory subCat ON fin.Sub=convert(varchar,Subcat.id)
                      LEFT JOIN statementCategories sc ON subCat.category=convert(varchar,sc.id)
                      where fin.subaccount&lt;&gt;1 and sc.statement='Income Statement' order by Ordering,sc.Category,subCat.SubType,Statement
                    </CommandText>
