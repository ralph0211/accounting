            <MainSource>
              <DbSource ConnectionRef="Constring (Web.config)" DbObjectType="Unknown" FillMethodModifier="Public" FillMethodName="Fill" GenerateMethods="Both" GenerateShortCommands="false" GeneratorGetMethodName="GetData" GeneratorSourceName="Fill" GetMethodModifier="Public" GetMethodName="GetData" QueryType="Rowset" ScalarCallRetval="System.Object, mscorlib, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" UseOptimisticConcurrency="false" UserGetMethodName="GetData" UserSourceName="Fill">
                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>
                      /*(select fin.[Type],fin.Sub,subCat.SubType,sc.Category,bsi.ItemName as [Statement], AccNo,tblTrxn.SubAccNo,AccountName as [Description],-(  case when (Bal) &lt;=  0 then Bal else null end)  as 'Debit', case when (Bal)  &gt;0 then Bal  end as Credit,tblTrxn.Bal*-1 as Balance,subcat.ordering as CatOrder,bsi.Ordering ,fin.Ordering as finOrder from (
                      select  case when ISNUMERIC(Account)=1 then [AccountPrefix] when ISNUMERIC(Account)=0 then left(account,case when  CHARINDEX('/', Account ) = 0 then LEN(Account) else CHARINDEX('/', Account) -1 end) end as AccNo,case when ISNUMERIC(Account)=1 OR left(account,case when  CHARINDEX('/', Account ) = 0 then LEN(Account) else CHARINDEX('/', Account) -1 end )=[AccountPrefix] then '2' ELSE right(account,len(account)-CHARINDEX('/',account)) end as SubAccNo,sum(debit) as Debit,sum(credit) as Credit,sum(credit)-sum(debit) as Bal from Accounts_Transactions join ParaInternalControls pic on 1=1 where TrxnDate &lt;= @tDate group by case when ISNUMERIC(Account)=1 then [AccountPrefix] when ISNUMERIC(Account)=0 then left(account,case when  CHARINDEX('/', Account ) = 0 then LEN(Account) else CHARINDEX('/', Account) -1 end ) end,case when ISNUMERIC(Account)=1 OR left(account,case when  CHARINDEX('/', Account ) = 0 then LEN(Account) else CHARINDEX('/', Account) -1 end )=[AccountPrefix] then '2' ELSE right(account,len(account)-CHARINDEX('/',account)) end
                      )tblTrxn left join
                      tbl_FinancialAccountsCreation fin on tblTrxn.AccNo=fin.MainAccount AND tblTrxn.SubAccNo=fin.SubAccount
                      LEFT JOIN tbl_FinancialCategory subCat ON fin.Sub=convert(varchar,Subcat.id)
                      LEFT JOIN statementCategories sc ON subCat.category=convert(varchar,sc.id)
                      LEFT JOIN balancesheetitems bsi ON convert(varchar,fin.BSItemId)=convert(varchar,bsi.id)
                      where fin.subaccount&lt;&gt;'1' and sc.Statement='Balance Sheet')--  order by subCat.Ordering,bsi.ordering,sc.Category,subCat.SubType,Statement

                      union

                      select 'Balance Sheet' AS [Type],'3' as [Sub],'Equity' as [SubType],'Equity and Liabilities' as [Category],'Current Earnings' as [Statement],'' as [AccNo],'0' AS [SubAccNo],'Current Year Profits' as [Description],SUM(balance) as [Debit],0 AS [Credit],sum(q.ActBal) as [Balance],3 as CatOrder,311 as ordering,9999999 as finOrder from (
                      select fin.[Type],fin.Sub,subCat.SubType,sc.Category,sc.Statement, AccNo,tblTrxn.SubAccNo,AccountName as [Description],
