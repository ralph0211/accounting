 (SELECT AccountNo,sum(BudgetedAmt) as BudgetedAmt FROM budget where convert(varchar(7), CONVERT(DATE,'01 '+BudgetMonth), 126)&gt;=convert(varchar(7), CONVERT(DATE,'01 '+@fMonth), 126) AND convert(varchar(7), CONVERT(DATE,'01 '+BudgetMonth), 126)&lt;=convert(varchar(7), CONVERT(DATE,'01 '+@tMonth), 126) GROUP BY AccountNo) b
 LEFT JOIN
 (SELECT Account,SUM(Debit-Credit) as Bal from Accounts_Transactions t JOIN Budget bb ON t.Account=bb.AccountNo where convert(varchar(7), CONVERT(DATE,t.TrxnDate), 126)&gt;=convert(varchar(7), CONVERT(DATE,'01 '+@fMonth), 126) AND convert(varchar(7), CONVERT(DATE,t.TrxnDate), 126)&lt;=convert(varchar(7), CONVERT(DATE,'01 '+@tMonth), 126) GROUP BY Account) a
 ON b.AccountNo=a.Account
 LEFT JOIN CashbookAccounts ca ON b.AccountNo=ca.AccountNo
 LEFT JOIN DebtorAccounts da ON b.AccountNo=da.AccountNo
 LEFT JOIN CreditorAccounts cr ON b.AccountNo=cr.AccountNo
 LEFT JOIN tbl_FinancialAccountsCreation f ON b.AccountNo=f.MainAccount</CommandText>
