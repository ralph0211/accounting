SELECT bs.*,fin.AccountName FROM (
	SELECT fac.Type,fac.Sub,subCat.SubType,sc.Category,sc.Statement,k.*,isnull(isnull(ISNULL(ISNULL(csh.MainAccount,crd.MainAccount),dbt.MainAccount),case when k.Account LIKE 'SW/%' THEN 'SW' ELSE Account END),fac.MainAccount) as [AccNo],ISNULL(ISNULL(ISNULL(ISNULL(csh.AccountName,crd.AccountName),dbt.AccountName),isnull(cd.FORENAMES,'') +' '+isnull(cd.SURNAME,'')),fac.AccountName) as [Description],CASE WHEN k.Balance&gt;0 THEN k.Balance ELSE 0 END as Debit,CASE WHEN k.Balance&gt;0 THEN 0 ELSE k.Balance END as Credit,subCat.Ordering as CatOrder,fac.Ordering as finOrder FROM
	 (SELECT acct.Account,SUM(acct.Debit-acct.Credit) as Balance from Accounts_Transactions acct WHERE acct.TrxnDate&lt;=@tDate GROUP BY acct.Account)k
	 LEFT JOIN CashbookAccounts csh ON k.Account=csh.AccountNo
	 LEFT JOIN CreditorAccounts crd ON k.Account=crd.AccountNo
	 LEFT JOIN DebtorAccounts dbt ON k.Account=dbt.AccountNo
	 LEFT JOIN CUSTOMER_DETAILS cd on k.Account=cd.CUSTOMER_NUMBER
	LEFT JOIN tbl_FinancialAccountsCreation fac ON isnull(isnull(ISNULL(ISNULL(csh.MainAccount,crd.MainAccount),dbt.MainAccount),case when k.Account LIKE 'SW/%' THEN 'SW' ELSE Account END),fac.MainAccount)=fac.MainAccount-- OR case when ISNUMERIC(Account)=1 then 'SW' when ISNUMERIC(Account)=0 then SUBSTRING(trxn.Account,0,3) end=fac.MainAccount
 LEFT JOIN tbl_FinancialCategory subCat ON fac.Sub=CONVERT(VARCHAR,subCat.ID)
 LEFT JOIN StatementCategories sc ON subCat.Category=CONVERT(VARCHAR,sc.id)
 WHERE sc.Statement='BS') bs
 JOIN tbl_FinancialAccountsCreation fin ON accno=fin.MainAccount
 union
 SELECT 'BS',3 as Sub,'Equity' as SubType,'Equity and Liabilities' as Category,'BS' as Statement,'902' as Account,SUM(t.Debit)-SUM(t.Credit) as Balance,'902' as AccNo,'Balance from Income Statement' as Description, SUM(t.Debit) as Debit,SUM(t.Credit) as Credit,999 as CatOrder,9999 as finOrder,'Current Year Earnings' as AccountName FROM (
SELECT trxn.TrxnDate,fac.[Type],fac.Sub,subCat.SubType,sc.Category,sc.Statement,trxn.Account,isnull(trxn.TrxnAccountName,fac.AccountName) as [Description],trxn.Debit,trxn.Credit FROM 
 (SELECT acct.*,ISNULL(ISNULL(ISNULL(csh.MainAccount,crd.MainAccount),dbt.MainAccount),fin.MainAccount) as [MainAccount],ISNULL(ISNULL(ISNULL(csh.AccountName,crd.AccountName),dbt.AccountName),fin.AccountName) as [TrxnAccountName] FROM Accounts_Transactions acct LEFT JOIN CashbookAccounts csh ON acct.Account=csh.AccountNo LEFT JOIN CreditorAccounts crd ON acct.Account=crd.AccountNo LEFT JOIN DebtorAccounts dbt ON acct.Account=dbt.AccountNo left join tbl_FinancialAccountsCreation fin on acct.Account=fin.MainAccount)
 trxn LEFT JOIN tbl_FinancialAccountsCreation fac ON trxn.MainAccount=fac.MainAccount OR case when ISNUMERIC(Account)=1 then 'SW' when ISNUMERIC(Account)=0 then SUBSTRING(trxn.Account,0,3)/* left(account,CHARINDEX('/',account)-1)*/ end=fac.MainAccount
 LEFT JOIN tbl_FinancialCategory subCat ON fac.Sub=CONVERT(VARCHAR,subCat.ID)
 LEFT JOIN StatementCategories sc ON subCat.Category=CONVERT(VARCHAR,sc.id)
 WHERE sc.Statement='PL' AND trxn.TrxnDate&lt;=@tDate
 )t
 ORDER BY bs.Category,CatOrder,finOrder</CommandText>
