            <MainSource>
              <DbSource ConnectionRef="Constring (Web.config)" DbObjectType="Unknown" FillMethodModifier="Public" FillMethodName="FillByDate" GenerateMethods="Both" GenerateShortCommands="false" GeneratorGetMethodName="GetDataByDate" GeneratorSourceName="FillByDate" GetMethodModifier="Public" GetMethodName="GetDataByDate" QueryType="Rowset" ScalarCallRetval="System.Object, mscorlib, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" UseOptimisticConcurrency="false" UserGetMethodName="GetDataByDate" UserSourceName="FillByDate">
                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>
                      /*select ac.TrxnDate,SUM(ac.Debit) as Debit,qa.CUSTOMER_NUMBER,qa.SURNAME,qa.FORENAMES,qa.IDNO,qa.ADDRESS,qa.FIN_TENOR,isnull(qa.FIN_AMT,0) as FIN_AMT,qa.FIN_ADMIN,am.PAYMENT,tblBal.Balance from Accounts_Transactions ac join QUEST_APPLICATION qa on ac.refrence=convert(varchar,qa.id) join AMORTIZATION_SCHEDULE am on qa.id=am.loanid join (select refrence as loanId,sum(Debit-Credit) as Balance from accounts_transactions ac where account like '213/%' and TrxnDate&lt;=@tDate group by refrence having sum(Debit-Credit)&gt;0) tblBal on convert(varchar,qa.id)=tblBal.loanId where (ac.account=qa.CUSTOMER_NUMBER or ac.Other=qa.CUSTOMER_NUMBER) and ac.account like '213/%' and am.PAYMENT_NO=1 and ac.trxndate&lt;=@tDate GROUP BY ac.TrxnDate,qa.CUSTOMER_NUMBER,qa.SURNAME,qa.FORENAMES,qa.IDNO,qa.ADDRESS,qa.FIN_TENOR,qa.FIN_AMT,qa.FIN_ADMIN,am.PAYMENT,tblBal.Balance*/

                      /*select ac.TrxnDate,SUM(ac.Debit) as Debit,qa.CUSTOMER_NUMBER,qa.SURNAME,qa.FORENAMES,qa.IDNO,qa.ADDRESS,qa.FIN_TENOR,qa.FIN_AMT,qa.FIN_ADMIN,am.PAYMENT,tblBal.Balance from Accounts_Transactions ac
                      join
                      QUEST_APPLICATION qa on ac.refrence=convert(varchar,qa.id)
                      JOIN [ParaInternalControls] pic on 1=1
                      join AMORTIZATION_SCHEDULE am on qa.id=am.loanid
                      join (select refrence as loanId,sum(Debit-Credit) as Balance from accounts_transactions ac JOIN [ParaInternalControls] pic on 1=1 where account like pic.AccountPrefix+pic.AccountSeparator+'%' and TrxnDate&lt;=@tDate group by refrence having sum(Debit-Credit)&gt;0) tblBal on convert(varchar,qa.id)=tblBal.loanId
                      where (ac.account=qa.CUSTOMER_NUMBER or ac.Other=qa.CUSTOMER_NUMBER) and ac.account like pic.AccountPrefix+pic.AccountSeparator+'%' and am.PAYMENT_NO=1 and ac.trxndate&lt;=@tDate
                      GROUP BY ac.TrxnDate,qa.CUSTOMER_NUMBER,qa.SURNAME,qa.FORENAMES,qa.IDNO,qa.ADDRESS,qa.FIN_TENOR,qa.FIN_AMT,qa.FIN_ADMIN,am.PAYMENT,tblBal.Balance

                      select ac.TrxnDate,SUM(ac.Debit) as Debit,qa.CUSTOMER_NUMBER,qa.SURNAME,qa.FORENAMES,qa.IDNO,qa.ADDRESS,qa.FIN_TENOR,qa.FIN_AMT,qa.FIN_ADMIN,am.PAYMENT,tblBal.Balance,tblCapRep.CapRep from Accounts_Transactions ac
                      join
                      QUEST_APPLICATION qa on ac.refrence=convert(varchar,qa.id)
                      JOIN [ParaInternalControls] pic on 1=1
                      join AMORTIZATION_SCHEDULE am on qa.id=am.loanid
                      join (select refrence as loanId,sum(Debit-Credit) as Balance from accounts_transactions ac JOIN [ParaInternalControls] pic on 1=1 where account like pic.AccountPrefix+pic.AccountSeparator+'%' and TrxnDate&lt;=@tDate group by refrence having sum(Debit-Credit)&gt;0) tblBal on convert(varchar,qa.id)=tblBal.loanId
                      left join (select refrence,sum(ac.Credit-ac.Debit) as CapRep from accounts_transactions ac JOIN [ParaInternalControls] pic on 1=1 where account like pic.AccountPrefix+pic.AccountSeparator+'%' and TrxnDate&lt;=@tDate AND ac.Description='Capital Repayment' group by refrence) tblCapRep on convert(varchar,qa.id)=tblCapRep.refrence
                      where (ac.account=qa.CUSTOMER_NUMBER or ac.Other=qa.CUSTOMER_NUMBER) and ac.account like pic.AccountPrefix+pic.AccountSeparator+'%' and am.PAYMENT_NO=1 and ac.trxndate&lt;=@tDate
                      GROUP BY ac.TrxnDate,qa.CUSTOMER_NUMBER,qa.SURNAME,qa.FORENAMES,qa.IDNO,qa.ADDRESS,qa.FIN_TENOR,qa.FIN_AMT,qa.FIN_ADMIN,am.PAYMENT,tblBal.Balance,tblCapRep.CapRep*/

                      select *,((select isnull(sum(w.payment),0) from AMORTIZATION_SCHEDULE w where w.LOANID =qa.ID and w.PAYMENT_DATE &lt;=@tDate)-(select isnull(sum(n.totalamount),0) from Repayments n where n.LoanID =qa.ID and n.TrxnDate &lt;= @tDate)) as arrears,isnull(qa.gender,qa.directorgender) as Genderr from Accounts_Transactions ac join QUEST_APPLICATION qa
                      on ac.refrence=qa.id join AMORTIZATION_SCHEDULE am on qa.id=am.loanid join
                      (select refrence as loanId,sum(Debit-Credit) as Balance from accounts_transactions ac where Category in
                      ('Loan Disbursement','Loan Repayment') and account in (select t.customer_number from QUEST_APPLICATION t) and TrxnDate&lt;=@tDate
                      group by refrence having sum(Debit-Credit)&gt;0) tblBal on qa.id=tblBal.loanId where ac.Category='Loan Disbursement' and ac.[Type] like '%system%' and (ac.account=qa.CUSTOMER_NUMBER or ac.Other=qa.CUSTOMER_NUMBER) and ac.account in (select t.customer_number from QUEST_APPLICATION t) and am.PAYMENT_NO=1 and ac.trxndate&lt;=@tDate
                    </CommandText>
