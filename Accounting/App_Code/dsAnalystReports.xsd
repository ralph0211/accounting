                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT 'Maturity Profile' as Chartt,
    CASE
        WHEN DATEDIFF(DAY,GETDATE(),am.PAYMENT_DATE)&gt;=0 and DATEDIFF(DAY,GETDATE(),am.PAYMENT_DATE)&lt;=30 THEN '1 - 30 days'
        WHEN DATEDIFF(DAY,GETDATE(),am.PAYMENT_DATE)&gt;30 AND DATEDIFF(DAY,GETDATE(),am.PAYMENT_DATE)&lt;=60 THEN '31 - 60 days'
        WHEN DATEDIFF(DAY,GETDATE(),am.PAYMENT_DATE)&gt;60 and DATEDIFF(DAY,GETDATE(),am.PAYMENT_DATE)&lt;=90 THEN '61 - 90 days'
        WHEN DATEDIFF(DAY,GETDATE(),am.PAYMENT_DATE)&gt;90 AND DATEDIFF(DAY,GETDATE(),am.PAYMENT_DATE)&lt;=180 THEN '91 - 180 days'
        WHEN DATEDIFF(DAY,GETDATE(),am.PAYMENT_DATE)&gt;180 THEN 'More than 180 days'
    END As [MatRange],
	SUM(isnull(am.PRINCIPAL,0)) as PrincipalMaturity,
	SUM(isnull(am.INTEREST,0)) as InterestMaturity
    FROM QUEST_APPLICATION qa JOIN AMORTIZATION_SCHEDULE am on qa.ID=am.LOANID
	WHERE am.PAYMENT_DATE&gt;=GETDATE()
GROUP BY
    CASE
        WHEN DATEDIFF(DAY,GETDATE(),am.PAYMENT_DATE)&gt;=0 and DATEDIFF(DAY,GETDATE(),am.PAYMENT_DATE)&lt;=30 THEN '1 - 30 days'
        WHEN DATEDIFF(DAY,GETDATE(),am.PAYMENT_DATE)&gt;30 AND DATEDIFF(DAY,GETDATE(),am.PAYMENT_DATE)&lt;=60 THEN '31 - 60 days'
        WHEN DATEDIFF(DAY,GETDATE(),am.PAYMENT_DATE)&gt;60 and DATEDIFF(DAY,GETDATE(),am.PAYMENT_DATE)&lt;=90 THEN '61 - 90 days'
        WHEN DATEDIFF(DAY,GETDATE(),am.PAYMENT_DATE)&gt;90 AND DATEDIFF(DAY,GETDATE(),am.PAYMENT_DATE)&lt;=180 THEN '91 - 180 days'
        WHEN DATEDIFF(DAY,GETDATE(),am.PAYMENT_DATE)&gt;180 THEN 'More than 180 days'
    END
ORDER BY
    MatRange</CommandText>
                    <Parameters />
                  </DbCommand>
