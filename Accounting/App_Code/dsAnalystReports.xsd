                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT format(tblBal.Balance, 'n') AS Balance, format(qa.FIN_AMT, 'n') AS LoanAmount, CONVERT (varchar, ISNULL(qa.MaturityDate, am.PAYMENT_DATE), 106) AS MaturityDate, qa.SURNAME + ' ' + qa.FORENAMES AS Name, format(0, 'n') AS PrincipalBalance, qa.ADDRESS, qa.BRANCH_NAME AS Branch, qa.CUSTOMER_NUMBER, qa.FinProductType, qa.Sector FROM AMORTIZATION_SCHEDULE AS am INNER JOIN QUEST_APPLICATION AS qa ON am.LOANID = qa.ID AND qa.FIN_TENOR = am.PAYMENT_NO INNER JOIN (SELECT Account, Refrence, SUM(Debit - Credit) AS Balance FROM Accounts_Transactions AS acct WHERE (Account LIKE '213/%') GROUP BY Account, Refrence) AS tblBal ON tblBal.Refrence = CONVERT (varchar, qa.ID) WHERE (am.PAYMENT_DATE &gt;= @sDate) AND (am.PAYMENT_DATE &lt;= @eDate)</CommandText>
