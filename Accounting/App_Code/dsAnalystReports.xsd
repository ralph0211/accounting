                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT qa.SURNAME+' '+qa.FORENAMES as [Name],[Address],qa.CUSTOMER_NUMBER,qa.FinProductType,qa.Sector,qa.BRANCH_NAME as [Branch],convert(varchar,isnull(qa.MaturityDate,am.PAYMENT_DATE),106) as [MaturityDate],format(qa.FIN_AMT,'n') as [LoanAmount],format(0,'n') as [PrincipalBalance],format(tblBal.Balance,'n') as [Balance] FROM AMORTIZATION_SCHEDULE am JOIN QUEST_APPLICATION qa ON am.LOANID=qa.ID AND qa.FIN_TENOR=am.PAYMENT_NO JOIN (SELECT acct.Account,acct.Refrence,sum(acct.Debit-acct.Credit) as [Balance] from Accounts_Transactions acct where acct.Account in (select customer_number from QUEST_APPLICATION) GROUP BY acct.Account,acct.Refrence) tblBal ON tblBal.Refrence=convert(varchar,qa.ID)</CommandText>
