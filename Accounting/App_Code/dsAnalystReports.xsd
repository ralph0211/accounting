          <TableAdapter BaseClass="System.ComponentModel.Component" DataAccessorModifier="AutoLayout, AnsiClass, Class, Public" DataAccessorName="BranchSummaryTableAdapter" GeneratorDataComponentClassName="BranchSummaryTableAdapter" Name="BranchSummary" UserDataComponentName="BranchSummaryTableAdapter">
            <MainSource>
              <DbSource ConnectionRef="Constring (Web.config)" DbObjectType="Unknown" FillMethodModifier="Public" FillMethodName="Fill" GenerateMethods="Both" GenerateShortCommands="false" GeneratorGetMethodName="GetData" GeneratorSourceName="Fill" GetMethodModifier="Public" GetMethodName="GetData" QueryType="Rowset" ScalarCallRetval="System.Object, mscorlib, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" UseOptimisticConcurrency="false" UserGetMethodName="GetData" UserSourceName="Fill">
                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT
	ISNULL(tblDisb.Branch, tblPend.Branch) AS [Branch]
	,ISNULL(tblDisb.NoDisbursements,0) as NoDisbursements
	,ISNULL(tblDisb.SumDisbursements,0) as SumDisbursements
	,ISNULL(tblPend.NoPending,0) as NoPending
	,ISNULL(tblPend.SumPending,0) as SumPending
FROM (SELECT
		qa.BRANCH_NAME AS [Branch]
		,isnull(COUNT(id),0) AS [NoDisbursements]
		,SUM(isnull(qa.FIN_AMT,0)) AS [SumDisbursements]
	FROM QUEST_APPLICATION qa
	WHERE qa.DISBURSED_DATE IS NOT NULL
	GROUP BY qa.BRANCH_NAME) tblDisb
LEFT JOIN (SELECT
		qa.BRANCH_NAME AS [Branch]
		,isnull(COUNT(qa.id),0) AS [NoPending]
		,SUM(isnull(qa.FIN_AMT,0)) AS [SumPending]
	FROM QUEST_APPLICATION qa
	JOIN paraapprovalstages pas
		ON qa.SEND_TO = CONVERT(VARCHAR, pas.roleid)
		AND qa.ApprovalNumber = pas.stageorder - 1
	WHERE pas.StageAction = 'Disbursement'
	AND STATUS &lt;&gt; 'REJECTED'
	GROUP BY qa.BRANCH_NAME) tblPend
	ON tblDisb.Branch = tblPend.Branch</CommandText>
                    <Parameters />
                  </DbCommand>
                </SelectCommand>
              </DbSource>
            </MainSource>
