                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>select * from
((select count(BNCH_CODE) as NoBranches from BNCH_DETAILS) b
left join
(select count(qa.ID) as NoOutstandingLoans,count(distinct customer_number) as NoActiveClients from QUEST_APPLICATION qa left join  BNCH_DETAILS bd on qa.BRANCH_CODE=bd.BNCH_CODE left join (select Refrence,sum(debit-credit) as Bal from Accounts_Transactions where ISNUMERIC(refrence)=1 and account like '213%' and trxndate&lt;=@tDate group by Refrence) t on qa.id=t.refrence where t.bal&gt;0 and qa.DISBURSED_DATE&gt;=@fDate and qa.DISBURSED_DATE&lt;=@tDate and bd.Location='Rural') r on 1=1
left join
(select count(id) as NoBorrowingGroups from CUSTOMER_DETAILS where [CUSTOMER_TYPE]='Group') g on 1=1
left join
(select count(qa.ID) as NoOutstandingLoansGroups,count(distinct customer_number) as NoActiveClientsGroups from QUEST_APPLICATION qa left join  BNCH_DETAILS bd on qa.BRANCH_CODE=bd.BNCH_CODE left join (select Refrence,sum(debit-credit) as Bal from Accounts_Transactions where ISNUMERIC(refrence)=1 and account like '213%' and trxndate&lt;=@tDate group by Refrence) t on qa.id=t.refrence where t.bal&gt;0 and qa.DISBURSED_DATE&gt;=@fDate and qa.DISBURSED_DATE&lt;=@tDate and CUSTOMER_TYPE='Group') gd on 1=1
left join
(select count(qa.ID) as NoOutstandingLoansIndiv,count(distinct customer_number) as NoActiveClientsIndiv from QUEST_APPLICATION qa left join  BNCH_DETAILS bd on qa.BRANCH_CODE=bd.BNCH_CODE left join (select Refrence,sum(debit-credit) as Bal from Accounts_Transactions where ISNUMERIC(refrence)=1 and account like '213%' and trxndate&lt;=@tDate group by Refrence) t on qa.id=t.refrence where t.bal&gt;0 and qa.DISBURSED_DATE&gt;=@fDate and qa.DISBURSED_DATE&lt;=@tDate and CUSTOMER_TYPE='Individual') i on 1=1
left join
(select count(id) as NoLoans10000 from QUEST_APPLICATION where FIN_AMT&gt;10000 and DISBURSED_DATE&gt;=@fDate and DISBURSED_DATE&lt;=@tDate) ten on 1=1
left join
(select count(id) as NoLoans20000 from QUEST_APPLICATION where FIN_AMT&gt;20000 and DISBURSED_DATE&gt;=@fDate and DISBURSED_DATE&lt;=@tDate) twe on 1=1
left join
(select count(id) as NoFemale from QUEST_APPLICATION where DISBURSED_DATE&gt;=@fDate and DISBURSED_DATE&lt;=@tDate and CUSTOMER_TYPE='Individual' and GENDER='F') f on 1=1
left join
(select count(id) as NoIndivLoans from QUEST_APPLICATION where DISBURSED_DATE&gt;=@fDate and DISBURSED_DATE&lt;=@tDate and CUSTOMER_TYPE='Individual') ii on 1=1)</CommandText>
