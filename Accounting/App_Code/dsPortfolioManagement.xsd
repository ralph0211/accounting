          <TableAdapter BaseClass="System.ComponentModel.Component" DataAccessorModifier="AutoLayout, AnsiClass, Class, Public" DataAccessorName="OutreachTableAdapter" GeneratorDataComponentClassName="OutreachTableAdapter" Name="Outreach" UserDataComponentName="OutreachTableAdapter">
