  </connectionStrings>
  <system.net>
    <mailSettings>
      <smtp from="escrow360cms@gmail.com">
        <network host="smtp.gmail.com" port="587" userName="escrow360cms@gmail.com" password="pass123$" />
      </smtp>
    </mailSettings>
  </system.net>
  <!--
    For a description of web.config changes see http://go.microsoft.com/fwlink/?LinkId=235367.

    The following attributes can be set on the <httpRuntime> tag.
      <system.Web>
        <httpRuntime targetFramework="4.5" />
      </system.Web>
