    <webServices>
      <protocols>
        <add name="HttpGet" />
        <add name="HttpPost" />
      </protocols>
    </webServices>
    <globalization culture="en-ZW" uiCulture="en" requestEncoding="UTF-8" responseEncoding="UTF-8" fileEncoding="utf-8" />
